<Window x:Class="OlsParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OlsParser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnSaveToCsv" Content="saveToCsv" HorizontalAlignment="Left" Margin="320,119,0,0" VerticalAlignment="Top" Click="btnSaveToCsv_Click"/>
        <Button x:Name="btnSaveToBin" Content="saveToCs" HorizontalAlignment="Left" Margin="320,159,0,0" VerticalAlignment="Top" Width="58" Click="btnSaveToBin_Click" />
        <Button x:Name="btnSaveToXml" Content="saveToXml" HorizontalAlignment="Left" Margin="320,197,0,0" VerticalAlignment="Top" Width="58" Click="btnSaveToXml_Click"/>

    </Grid>
</Window>
